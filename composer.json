{
  "name": "odura/elasticquent",
  "type": "library",
  "description": "Maps Laravel Eloquent models to Elasticsearch types forked from elasticquent/elasticquent.",
  "keywords": [
    "elasticsearch",
    "eloquent",
    "laravel"
  ],
  "homepage": "https://github.com/odura/Elasticquent",
  "license": "MIT",
  "authors": [
    {
      "name": "Olivier Durand",
      "email": "olivier.durand@odura.fr",
      "homepage": "https://github.com/odura"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/database": "^5",
    "illuminate/config": "^5",
    "nesbot/carbon": "~2.16",
    "elasticsearch/elasticsearch": "^6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Elasticquent\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
